<AnchorPane id="AnchorPane" prefHeight="395.0" prefWidth="472.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.LoginController">
   <children>
      <Label alignment="CENTER" layoutX="51.0" layoutY="14.0" prefHeight="38.0" prefWidth="370.0" text="Inicio de sesión">
         <font>
            <Font size="24.0" />
