            <Image url="@../fotostrabajo/logo-sinfondo.png" />
         </image>
      </ImageView>
      <Text layoutX="61.0" layoutY="226.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Si aún no te has creado una cuenta ">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
