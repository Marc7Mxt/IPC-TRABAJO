      </Label>
      <Label layoutX="109.0" layoutY="262.0" prefHeight="18.0" prefWidth="75.0" text="Nickname" />
      <Label layoutX="106.0" layoutY="302.0" prefHeight="18.0" prefWidth="81.0" text="Contraseña" />
      <TextField fx:id="textfieldNicknameLogin" layoutX="192.0" layoutY="258.0" prefHeight="25.0" prefWidth="207.0" />
      <PasswordField fx:id="passfieldLogin" layoutX="192.0" layoutY="298.0" prefHeight="25.0" prefWidth="207.0" />
      <Button fx:id="botonRegistrarse" layoutX="285.0" layoutY="206.0" mnemonicParsing="false" onAction="#registrarseClicked" prefHeight="25.0" prefWidth="111.0" text="REGÍSTRATE">
